<resources>

    <array name="rainbow">
        <item>@color/colorPrimary</item>
        <item>@color/colorAccent</item>
        <item>@color/colorPrimaryDark</item>
    </array>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:endColor">#FF1A54CD</item>
        <item name="android:startColor">#FF3275FF</item>
        <item name="android:type">radial</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
    </style>

    <style name="ToolBarTheme"></style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SignInTxt">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FF162637</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="SignIn">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFA3B2CB</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="EnterYourMailId">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFA3B2CB</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ButtonTheme">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="DonTHaveAAccountOne">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFA3B2CB</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="DonTHaveAAccount">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FF3174FF</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="SignUpField">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFA3B2CB</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="ScanTxt">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FF132A58</item>
        <item name="android:textSize">26sp</item>
    </style>

    <style name="ScanSuggestion">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FF5B739B</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ScanSkip">
        <item name="android:fontFamily">@font/proxima_nova_semibold</item>
        <item name="android:textColor">#FFA3B2CB</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ProjectNameList">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF44515F</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="progressBarStyle" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:indeterminateDrawable">@drawable/progress_bar_drawable</item>
    </style>

    <style name="BuildingName">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF44515F</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="BuildingTask">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="BuildingFlat">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF7C8FAF</item>
        <item name="android:textSize">33sp</item>
    </style>

    <style name="BuildingFlatLevel">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF7C8FAF</item>
        <item name="android:textSize">7sp</item>
    </style>

    <style name="BuildingTabs">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF0787FF</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="BuildingFlatCardView">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF162637</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="PlanText">
        <item name="android:fontFamily">Proxima Nova</item>
        <item name="android:textColor">#FF44515F</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>
